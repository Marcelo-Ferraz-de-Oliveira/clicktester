{
  "manifest_version": 3,
  "name": "ClickTester",
  "version": "0.0.1",
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["https://*/*", "http://*/*"],
    "js": ["content.js"]
  }],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Click Tester",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [ ],
      "matches": ["https://*.youtube.com/*"]
    }
  ]
}
